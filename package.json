{
  "name": "gatsby-remark-images-insert-wrapper-attributes",
  "version": "1.0.0",
  "description": "Gatsby Transformer Remark plugin that inserts AST properties into the wrapper generated by Gatsby Remark Images",
  "main": "index.js",
  "jest": {
    "collectCoverage": true
  },
  "scripts": {
    "build": "babel src --out-dir ./ --source-maps",
    "watch": "babel src --out-dir ./ --source-maps --watch",
    "lint": "eslint --ext js ./ --cache",
    "test": "jest --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsproutsmedia/gatsby-remark-images-insert-wrapper-attributes.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-transformer-remark",
    "image-attributes",
    "gatsby-remark-images",
    "image-alignment"
  ],
  "author": "NB Walters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/newsproutsmedia/gatsby-remark-images-insert-wrapper-attributes/issues"
  },
  "homepage": "https://github.com/newsproutsmedia/gatsby-remark-images-insert-wrapper-attributes#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "jsdom": "^16.5.3",
    "unist-util-visit": "^2.0.3"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0",
    "gatsby-transformer-remark": ">2.0.0"
  }
}
